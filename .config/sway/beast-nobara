output "HDMI-A-3" {
    mode  1920x1080@60.0Hz
    pos 0 0
    transform 270
    scale 1.0
    scale_filter nearest
    adaptive_sync off
    dpms on
}
output "DP-2" {
    mode  3440x1440@99.982Hz
    pos 1080 0
    transform normal
    scale 1.0
    scale_filter nearest
    adaptive_sync off
    dpms on
}

workspace 0 output HDMI-A-3
workspace 1 output DP-2
